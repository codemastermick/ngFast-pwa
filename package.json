{
  "name": "ng-fast-pwa",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "test": "ng test --watch false --browsers ChromeHeadless",
    "lint": "ng lint",
    "lint:all": "ng lint --fix",
    "e2e": "ng e2e",
    "compile:server": "webpack --config webpack.server.config.js --progress --colors",
    "compile:translations": "ng xi18n --output-path translate",
    "compile:en": "ng build --prod --i18n-locale en --i18n-format xlf --i18n-file translate/messages.en.xlf --output-path=dist/en --baseHref /en/",
    "compile:fr": "ng build --prod --i18n-locale fr --i18n-format xlf --i18n-file translate/messages.fr.xlf --output-path=dist/fr --baseHref /fr/",
    "build-locale": "npm run compile:en && npm run compile:fr",
    "serve:ssr": "node dist/server",
    "serve:fr": "ng serve --configuration=fr",
    "build:ssr": "npm run build:client-and-server-bundles && npm run compile:server",
    "build:client-and-server-bundles": "ng build --prod && ng run ngFast-pwa:server:production",
    "test:coverage": "ng test --watch false --browsers ChromeHeadless --code-coverage",
    "test:upload": "cat ./coverage/ngFast-pwa/lcov.info | codacy-coverage --accountToken oQObWyuKknLkdKr7XDUF --username codemastermick --projectName ngFast-pwa",
    "next-update": "next-update -k true --tldr"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.0.0",
    "@angular/cdk": "^8.0.1",
    "@angular/common": "~8.0.0",
    "@angular/compiler": "~8.0.0",
    "@angular/core": "~8.0.0",
    "@angular/forms": "~8.0.0",
    "@angular/material": "^8.0.1",
    "@angular/platform-browser": "~8.0.0",
    "@angular/platform-browser-dynamic": "~8.0.0",
    "@angular/platform-server": "~8.0.0",
    "@angular/pwa": "^0.800.2",
    "@angular/router": "~8.0.0",
    "@angular/service-worker": "~8.0.0",
    "@nguniversal/express-engine": "v7.0.2",
    "@nguniversal/module-map-ngfactory-loader": "v7.0.2",
    "codacy-coverage": "^3.4.0",
    "codecov": "^3.5.0",
    "express": "^4.15.2",
    "hammerjs": "^2.0.8",
    "rxjs": "~6.4.0",
    "tslib": "^1.9.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.800.0",
    "@angular/cli": "~8.0.2",
    "@angular/compiler-cli": "~8.0.0",
    "@angular/language-service": "~8.0.0",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "^5.0.1",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "next-update": "^3.6.0",
    "protractor": "~5.4.0",
    "ts-loader": "^5.2.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "typescript": "~3.4.3",
    "webpack-cli": "^3.3.3"
  }
}
